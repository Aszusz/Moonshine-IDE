				xmlns:s="library://ns.adobe.com/flex/spark" 
				xmlns:mx="library://ns.adobe.com/flex/mx"
				label="{data.projectName}">
	<s:Label id="labelDisplay" fontWeight="bold" useHandCursor="true"
			 left="0"/>
	<s:Label text="{data.description}" left="{labelDisplay.width + 5}" 
			 maxWidth="{parent.width - labelDisplay.width}"/>
	<fx:Script>
	<![CDATA[
		override protected function set hovered(value:Boolean):void
		{
				
		}				
	]]>
</fx:Script>
</s:ItemRenderer>